<!DOCTYPE html>
<html lang="{{ app.request.locale|split('_')[0] }}">
    <head>
        <title>{{ get_env('BRAND')|humanize }}</title>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">

        {% block meta %}
            {% include "@SuluWebsite/Extension/seo.html.twig" with {
                "seo": extension.seo|default([]),
                "content": content|default([]),
                "localizations": localizations|default([]),
                "shadowBaseLocale": shadowBaseLocale|default(),
                "defaultLocale": app.request.locale
            } %}
        {% endblock %}

        {% block style %}
            <link rel="stylesheet" href="{{ asset('build/website/app.css') }}">

            <!-- Demo -->
            <link rel="stylesheet" href="{{ asset('build/demo/css/font-awesome.min.css') }}">
            <link rel="stylesheet" href="{{ asset('build/demo/css/icofont.css') }}">
            <link rel="stylesheet" href="{{ asset('build/demo/css/bootstrap.min.css') }}">
            <link rel="stylesheet" href="{{ asset('build/demo/vendors/revolution/css/settings.css') }}">
            <link rel="stylesheet" href="{{ asset('build/demo/vendors/revolution/css/layers.css') }}">
            <link rel="stylesheet" href="{{ asset('build/demo/vendors/revolution/css/navigation.css') }}">
            <link rel="stylesheet" href="{{ asset('build/demo/vendors/animate-css/animate.css') }}">
            <link rel="stylesheet" href="{{ asset('build/demo/vendors/magnific-popup/magnific-popup.css') }}">
            <link rel="stylesheet" href="{{ asset('build/demo/vendors/owl-carousel/assets/owl.carousel.min.css') }}">
            <link rel="stylesheet" href="{{ asset('build/demo/css/style.css') }}">
            <link rel="stylesheet" href="{{ asset('build/demo/css/responsive.css') }}">
            <link rel="stylesheet" href="{{ asset('build/demo/css/override.css') }}">

            <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
            <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
            <!--[if lt IE 9]>
            <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
            <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
            <![endif]-->
        {% endblock %}
    </head>
    <body>
    <!-- Header navigation -->
    {% include 'includes/header.html.twig' %}

    <!-- Content -->
    {% block content %}
    {% endblock %}

    <!-- Footer -->
    {% include 'includes/footer.html.twig' %}

    {% block javascripts %}
        <script src="{{ asset('build/website/runtime.js') }}"></script>
        <script src="{{ asset('build/website/app.js') }}"></script>
        <script src="{{ asset('build/website/main.js') }}"></script>
        <!-- Demo -->
        <script src="{{ asset('build/demo/js/jquery-2.2.4.js') }}"></script>
        <script src="{{ asset('build/demo/js/bootstrap.min.js') }}"></script>
        <script src="{{ asset('build/demo/vendors/revolution/js/jquery.themepunch.tools.min.js') }}"></script>
        <script src="{{ asset('build/demo/vendors/revolution/js/jquery.themepunch.revolution.min.js') }}"></script>
        <script src="{{ asset('build/demo/vendors/revolution/js/extensions/revolution.extension.video.min.js') }}"></script>
        <script src="{{ asset('build/demo/vendors/revolution/js/extensions/revolution.extension.slideanims.min.js') }}"></script>
        <script src="{{ asset('build/demo/vendors/revolution/js/extensions/revolution.extension.layeranimation.min.js') }}"></script>
        <script src="{{ asset('build/demo/vendors/revolution/js/extensions/revolution.extension.navigation.min.js') }}"></script>
        <script src="{{ asset('build/demo/vendors/parallax/jquery.parallax-scroll.js') }}"></script>
        <script src="{{ asset('build/demo/vendors/magnific-popup/jquery.magnific-popup.min.js') }}"></script>
        <script src="{{ asset('build/demo/vendors/owl-carousel/owl.carousel.min.js') }}"></script>
        <script src="{{ asset('build/demo/vendors/counterup/waypoints.min.js') }}"></script>
        <script src="{{ asset('build/demo/vendors/counterup/jquery.counterup.min.js') }}"></script>
        <script src="{{ asset('build/demo/vendors/flexslider/flex-slider.js') }}"></script>
        <script src="{{ asset('build/demo/vendors/flexslider/mixitup.js') }}"></script>
        <script src="{{ asset('build/demo/vendors/swiper/js/swiper.min.js') }}"></script>
        <script src="{{ asset('build/demo/vendors/flipster-slider/jquery.flipster.min.js') }}"></script>
        <script src="{{ asset('build/demo/vendors/nice-selector/jquery.nice-select.min.js') }}"></script>
        <script src="{{ asset('build/demo/js/theme.js') }}"></script>
    {% endblock %}
    </body>
</html>
